{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "enabledManagers": ["gomod"],
  "packageRules": [
    {
      "matchPackageNames": ["github.com/yoyoIgnitor/go-modules/module1"],
      "matchDatasources": ["git-tags"],
      "matchSourceUrlPrefixes": ["https://github.com/yoyoIgnitor/go-modules"],
      "versioning": "semver",
      "extractVersion": "module1-v(?<version>[0-9]+\\.[0-9]+\\.[0-9]+)",
      "allowedVersions": "^module1-v[0-9]+\\.[0-9]+\\.[0-9]+$"
    }
  ]
}