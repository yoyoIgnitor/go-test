{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "enabledManagers": ["gomod"],
  "packageRules": [
    {
      "matchPackageNames": [
        "github.com/yoyoIgnitor/go-modules/modules1"
      ],
      "enabled": true,
      "versioning": "regex:^module1-v[0-9]+\\.[0-9]+\\.[0-9]+$",
      "matchSourceUrlPrefixes": ["https://github.com/yoyoIgnitor/go-modules"],
      "matchDatasources": ["git-tags"],
      "allowedVersions": "^module1-v[0-9]+\\.[0-9]+\\.[0-9]+$"
    }
  ]
}


