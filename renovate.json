{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "enabledManagers": ["gomod"],
  "packageRules": [
    {
      "matchDatasources": ["git-tags"],
      "matchPackageNames": ["github.com/yoyoIgnitor/go-modules/module1"],
      "versioning": "semver",
      "extractVersion": "v(?<version>[0-9]+\\.[0-9]+\\.[0-9]+)"
    }
  ]
}


