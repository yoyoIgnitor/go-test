{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "enabledManagers": ["gomod"],
  "packageRules": [
    {
      "matchManagers": ["gomod"],
      "matchPackageNames": ["github.com/yoyoIgnitor/go-modules/module1"],
      "versionCompatibility": "^(module1-)?v(?<version>[\\d\\.]+)$",
      "versioning": "semver"
    }
  ]
}

