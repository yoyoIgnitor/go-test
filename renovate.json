{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "enabledManagers": ["gomod"],
  "prConcurrentLimit": 40,
  "prHourlyLimit": 0,
  "branchConcurrentLimit": 20,
  "regexManagers": [
    {
      "fileMatch": ["^go\\.mod$"],
      "matchStrings": [
        "github\\.com/yoyoIgnitor/go-modules/module1 (?<currentValue>module1-v[\\d\\.]+)"
      ],
      "datasourceTemplate": "git-tags",
      "depNameTemplate": "github.com/yoyoIgnitor/go-modules/module1",
      "lookupNameTemplate": "github.com/yoyoIgnitor/go-modules/module1",
      "versioningTemplate": "semver"
    }
  ]
}