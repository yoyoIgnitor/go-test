{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "enabledManagers": ["gomod"],
  "packageRules": [
    {
      "matchManagers": ["gomod"],
      "matchPackageNames": ["github.com/yoyoIgnitor/go-modules/module1"],
      "versionCompatibility": "^module1-(?<version>v[0-9]+\\.[0-9]+\\.[0-9]+)$",
      "extractVersion": "^module1-(?<version>v[0-9]+\\.[0-9]+\\.[0-9]+)$",
      "versioning": "semver"
    }
  ]
}

